Options -MultiViews
RewriteEngine On

<IfModule mod_rewrite.c>
  RewriteEngine On

  # Ne pas réécrire les fichiers/dossiers existants (images, assets, css, js, etc.)
  RewriteCond %{REQUEST_FILENAME} -f [OR]
  RewriteCond %{REQUEST_FILENAME} -d
  RewriteRule ^ - [L]

  # SPA fallback
  RewriteRule . /index.html [L]
</IfModule>